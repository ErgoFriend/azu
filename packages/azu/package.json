{
  "name": "azu",
  "version": "1.0.0",
  "description": "The azu CLI",
  "keywords": [
    "azu"
  ],
  "homepage": "https://github.com/azu/azu/tree/master/packages/azu/",
  "bugs": {
    "url": "https://github.com/azu/azu/issues"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/",
    "resources/"
  ],
  "bin": {
    "azu": "bin/azu.js"
  },
  "main": "lib/Azu.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/azu.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir lib --source-maps",
    "prepublish": "npm run --if-present build",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "watch": "babel src --out-dir lib --source-maps --watch"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4
  },
  "devDependencies": {
    "@types/node": "^10.0.6",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.5",
    "prettier": "^1.12.1"
  },
  "dependencies": {
    "ink": "^0.5.0",
    "ink-select": "^1.2.0",
    "ink-select-input": "^1.1.0",
    "meow": "^5.0.0",
    "opn": "^5.3.0",
    "term-img": "^2.1.0",
    "terminal-image": "^0.1.1"
  }
}
